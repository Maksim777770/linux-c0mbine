# mkinitcpio preset file for the '%PKGBASE%' package

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-%PKGBASE%"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio-%PKGBASE%.conf"
default_image="/boot/initramfs-ck-haswell-vfio.img"
#default_options=""

#fallback_config="/etc/mkinitcpio-%PKGBASE%.conf"
fallback_image="/boot/initramfs-ck-haswell-vfio-fallback.img"
fallback_options="-S autodetect"
